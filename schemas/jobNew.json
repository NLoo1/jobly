{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "title",
      "salary",
      "equity",
      "company_handle"
  ],
  "properties": {
      "title": {
          "type": "string",
          "default": "",
          "title": "The title Schema",
          "examples": [
              "Data Analyst"
          ]
      },
      "salary": {
          "type": "integer",
          "default": 0,
          "title": "The salary Schema",
          "examples": [
              60000
          ]
      },
      "equity": {
          "type": "number",
          "default": 0.0,
          "title": "The equity Schema",
          "examples": [
              0.9
          ]
      },
      "company_handle": {
          "type": "string",
          "default": "",
          "title": "The company_handle Schema",
          "examples": [
              "apple"
          ]
      }
  },
  "examples": [{
      "title": "Data Analyst",
      "salary": 60000,
      "equity": 0.9,
      "company_handle": "apple"
  }]
}